{
  "name": "visual-editor",
  "version": "0.1.0",
  "description": "Visual Editor for AI Site Generator",
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./injection": {
      "import": "./dist/injection/visual-edit-script.js",
      "types": "./dist/injection/visual-edit-script.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts injection/visual-edit-script.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts injection/visual-edit-script.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "visual-editor",
    "jsx",
    "tailwind",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zustand": "^4.4.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jsdom": "^27.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}
